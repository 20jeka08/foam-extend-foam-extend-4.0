FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiSolverDemo

deltaT          0.01;

writeControl    timeStep;

writeInterval   1;

startFrom       startTime;

startTime       0;

stopAt          noWriteNow;

endTime         0.1;

timeFormat      scientific;

timePrecision   6;


