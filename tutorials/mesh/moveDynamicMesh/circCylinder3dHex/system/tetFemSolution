FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      tetFemSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    motionU
    {
        solver          GMRES;
        nDirections     8;
        preconditioner  Cholesky;
        tolerance       1e-09;
        relTol          0.0001;
    }
}

