FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -2 0 0 0 0];

internalField       uniform 0;

referenceLevel      0;

boundaryField
{
    bottomWall
    {
        type fixedValue;
        value uniform 0.0;
    }
    topWall
    {
        type fixedValue;
        value uniform 0.0;
    }
    sideWall
    {
        type fixedValue;
        value uniform 0.0;
    }
}

