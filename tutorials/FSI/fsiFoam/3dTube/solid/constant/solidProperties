FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solidSolver unsTotalLagrangianSolid;

unsTotalLagrangianSolidCoeffs
{
    nCorrectors 200;

    convergenceTolerance 1e-7;
    relConvergenceTolerance 1e-3;

    nonLinear off;
    debug no;
}

unsIncrTotalLagrangianSolidCoeffs
{
    nCorrectors 200;

    convergenceTolerance 1e-7;
    relConvergenceTolerance 1e-3;

    nonLinear off;
    debug no;
}

