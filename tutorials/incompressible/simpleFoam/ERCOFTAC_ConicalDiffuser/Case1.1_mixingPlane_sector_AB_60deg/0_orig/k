FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.0184;

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform 2.0184;
    }

    outlet          
    {
        type            zeroGradient;
    }

    wallProlongation
    {
        type            zeroGradient;
    }

    wallDiffuser
    {
        type            zeroGradient;
    }

    statSwirlWallB_C
    {
        type            zeroGradient;
    }

    rotSwirlWallA_BB
    {
        type            zeroGradient;
    }

    B_UPSTREAM
    {
        type            mixingPlane;
    }

    B_DOWNSTREAM
    {
        type            mixingPlane;
    }
    sideWallA_BB_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallA_BB_cyclic2
    {
        type            cyclicGgi;
    }

}

