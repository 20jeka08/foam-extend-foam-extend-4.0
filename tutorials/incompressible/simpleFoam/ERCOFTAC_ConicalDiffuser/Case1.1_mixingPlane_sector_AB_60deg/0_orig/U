FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 11.6);

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform (0 0 11.6);

    }

    outlet          
    {
        type            zeroGradient;
    }

    wallProlongation
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    wallDiffuser
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    statSwirlWallB_C
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    rotSwirlWallA_BB
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    B_UPSTREAM
    {
        type            mixingPlane;
    }

    B_DOWNSTREAM
    {
        type            mixingPlane;
    }
    sideWallA_BB_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallA_BB_cyclic2
    {
        type            cyclicGgi;
    }

}

