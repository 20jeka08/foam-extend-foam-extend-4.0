    }

    wallProlongation
    {
        type            zeroGradient;
    }

    wallDiffuser
    {
        type            zeroGradient;
    }

    statSwirlWallB_C
    {
        type            zeroGradient;
    }

    rotSwirlWallA_BB
    {
        type            zeroGradient;
    }

    sideWallA_BB_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallA_BB_cyclic2
    {
        type            cyclicGgi;
    }

    B_UPSTREAM
    {
        type            ggi;
    }

    B_DOWNSTREAM
    {
        type            ggi;
    }

    sideWallB_C_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallB_C_cyclic2
    {
        type            cyclicGgi;
    }

    sideWallC_D_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallC_D_cyclic2
    {
        type            cyclicGgi;
    }

    sideWallD_E_cyclic1
    {
        type            cyclicGgi;
    }

    sideWallD_E_cyclic2
    {
        type            cyclicGgi;
    }

    sideWallE_F_cyclic1
    {
        type            cyclicGgi;
