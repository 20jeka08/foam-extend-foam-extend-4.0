FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    inlet1
    {
        type            fixedValue;
        value           uniform (0 1 0);
    }

    inlet2
    {
        type            fixedValue;
        value           uniform (0 -1 0);
    }

    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

