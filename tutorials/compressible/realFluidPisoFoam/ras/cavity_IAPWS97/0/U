FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    movingWall
    {
        type            fixedValue;
        // Field Value
        value           uniform (1 0 0);
    }

    fixedWalls
    {
        type            fixedValue;
        // Field Value
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

