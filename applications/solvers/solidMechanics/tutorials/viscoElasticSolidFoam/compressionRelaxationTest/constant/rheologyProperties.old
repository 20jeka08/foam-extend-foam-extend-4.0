    rho             rho  [1 -3 0 0 0 0 0]  1000;

    kDimensions [1 -1 -2 0 0 0 0];

    tauDimensions [0 0 1 0 0 0 0];

    size 5;

    k nonuniform List<scalar> 
    5
    ( 
       708.709
       1358.33
       5.80601
       307.687
        619.47  
    );

    tau nonuniform List<scalar>
    5
    (
        0
        0.223865
        5.80775
        16.0804
        199.512
    );

    nu nu [0 0 0 0 0 0 0] 0.45;
}

